  </executables>
  <bindings>
    <attributeValues IterBinding="ProductsU1Iterator" id="ProductName">
      <AttrNames>
        <Item Value="ProductName"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="ProductsU1Iterator" id="ProductDescription">
      <AttrNames>
        <Item Value="ProductDescription"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="ProductsU1Iterator" id="ProductPrice">
      <AttrNames>
        <Item Value="ProductPrice"/>
      </AttrNames>
    </attributeValues>
