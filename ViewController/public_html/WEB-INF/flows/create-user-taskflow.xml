<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="create-user-taskflow">
    <default-activity>CreateUserStart</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>backing_CreateUserStart</managed-bean-name>
      <managed-bean-class>view.backing.CreateUserStart</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
      <!--oracle-jdev-comment:managed-bean-jsp-link:1CreateUserStart.jsff-->
    </managed-bean>
    <view id="CreateUserStart">
      <page>/CreateUserStart.jsff</page>
    </view>
    <view id="view1"></view>
    <view id="view2"></view>
    <control-flow-rule id="__2">
      <from-activity-id>CreateUserStart</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>manager</from-outcome>
        <to-activity-id>view1</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <to-activity-id>view2</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
        <from-outcome>vp</from-outcome>
        <to-activity-id>view1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
