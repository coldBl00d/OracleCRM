<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="product-welcome">
    <default-activity>productWelcome</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>backing_productWelcome</managed-bean-name>
      <managed-bean-class>view.backing.ProductWelcome</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
      <!--oracle-jdev-comment:managed-bean-jsp-link:1productWelcome.jsff-->
    </managed-bean>
    <view id="productWelcome">
      <page>/productWelcome.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
